{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "no-console": [
            true,
            "logs"
        ]
    },
    "rules": {
        "trailing-comma": [
            false
        ],
        "no-console": [
            true,
            "logs"
        ],
        "variable-name":false,
        "no-var-requires":false,
        "prefer-for-of": false,
        "forin": false
    },
    "rulesDirectory": []
}